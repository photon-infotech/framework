<section class="content_main proj_list">
	{{#if projectlist}}
		<div class="fixed-table-container">
        <div class="header-background"> </div>
        <div class="fixed-table-container-inner"> 
		<table class="table table-striped table_border table-hover table-bordered" id="projectlist" cellpadding="0" cellspacing="0" border="0">
              <thead class="height_th">
                 <tr>
                  <th><div class="th-inner" data-i18n="projectlist.label.name"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.description"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.technology"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.report"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.repository"></div></th>
                  <th><div class="th-inner" data-i18n="commonlabel.label.action"></div></th>                  
                  <th><div class="th-inner">&nbsp;</div></th>
                </tr>
              </thead>
              <tbody>
                  {{#each projectlist}}
                      <tr class="proj_title"> 
                        <td colspan="4">{{name}}</td>  
                        <td colspan="3" align="right" class="delimages">
                        	<a href="javascript:void(0)" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip"  data-original-title="Edit Project">
                        		<img name="editproject" key="{{id}}" projectcode="{{projectCode}}" projectId="{{id}}" src="themes/default/images/Phresco/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    		</a> 
                    		{{#if ../userPermissions.manageApplication}}
	                    		<a name="{{id}}" appIds="{{#csvAppIds appInfos}}{{/csvAppIds}}" subModuleIds="{{#allSubModuleIds appInfos}}{{/allSubModuleIds}}" class="tooltiptop del_project" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete {{name}}">
	                    			<img src="themes/default/images/Phresco/delete_row_icon.png" class="projectlist_delete" width="29" height="23" border="0" alt="0">
	                			</a>
                			{{else}}
                				<img src="themes/default/images/Phresco/delete_row_icon_off.png" width="29" height="23" border="0" alt="0">
                			{{/if}}

                            <div id="{{id}}" class="dyn_popup deleteproj_msg"> Are you sure to delete this ?
                                <div>
                                    <input type="button" value="Yes" data-i18n="[value]common.btn.yes" class="btn btn_style" name="holeDelete" deleteAppname="{{name}}">
                                    <input type="button" value="No" data-i18n="[value]common.btn.no" class="btn btn_style dyn_popup_close">
                                </div>
                            </div>
                        </td>
                      </tr>
					         {{#each appInfos}}
                      <tr class="{{appDirName}}" techid="{{techInfo.id}}">
                        <td width="25%"><a name="editApplication" appid="{{id}}" modules="{{#hasModules modules}}{{/hasModules}}" href="javascript:;">{{name}}</a></td>
                        <td width="25%">{{#descriptionshow description}}{{/descriptionshow}}</td>
                        <td width="25%">{{techInfo.name}}</td>
                       <td class="list_img">
					   
					   {{#pdfpermissiondata id ../../userPermissions}}{{/pdfpermissiondata}}
					   
              <div id="pdf_report_{{id}}" class="dyn_popup pdf_report">
              <div id="noReport_{{id}}" class="alert noReport hideContent" style="text-align: center;"></div>
				  <table class="hideContent table" name="pdfHeader_{{id}}" show="true">
						  <thead>  
							<tr>
								<th class="" data-i18n="commonlabel.label.name"></th>
								<th class="align" data-i18n="commonlabel.label.time"></th>
								<th class="align1" data-i18n="commonlabel.label.download"></th>
								<th class="" data-i18n="commonlabel.label.delete"></th>
							</tr>
						  </thead>			
				  </table>	
				  <div class = "pdfheight">
				  <table class="existing_report" width="100%" border="0" cellspacing="0" cellpadding="5">
						<tbody name="generatedPdfs_{{id}}" class="scroll">
						</tbody>    
				  </table>
				  </div>	  
                <form id="pdfReport">
                   <table class="report_type">
                    <tbody>
                        <tr>
                        	<td data-i18n="commonlabel.label.reporttype"></td>
                        	<td>
                            	<select name="reportDataType" class="reportDataType">
                                	<option value="detail" data-i18n="projectlist.label.detailedReport"></option>
                                	<option value="crisp" data-i18n="projectlist.label.overallReport"></option>
                            	</select>
                        	</td>
                    	</tr>
                         <tr>
                        	<td data-i18n="commonlabel.label.pdfreportname"></td>
                            <td>
                               <input type="text" data-i18n="[placeholder]projectlist.label.pdfreportname" maxlength="30" title="30 Characters only" name="pdfName" class="pdfName">
                            </td>
                        </tr>
                    </tbody>
                   </table>
                   <div class="popuploading" id="pdfReportLoading_{{id}}">&nbsp</div>
                <div class="flt_right">
                {{#if ../../userPermissions.managePdfReports}}
                 	<input type="button" fromPage="All"  appDirName="{{appDirName}}" value="Generate" data-i18n="[value]projectlist.label.generate" class="btn btn_style" name="generate" appId="{{id}}" projectId = "{{../id}}">
                 {{else}}
                 	<input type="button" disabled=true value="Generate"  class="btn btn_style" appId="{{id}}" projectId = "{{../id}}">
                  {{/if}}
                 <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
               </div>
               <input type="hidden" name="fromPage" value="All" data-i18n="[value]common.btn.all"/>
               </form>
                <!--pdf_report div ends here -->  
             </div> 
                  </td>
                        <td class="list_img">
                        {{#permissiondata id ../../userPermissions}}{{/permissiondata}}
                        {{#updatepermissiondata id ../../userPermissions}}{{/updatepermissiondata}}
                  <div id="addRepo_{{id}}" class="add_repo dyn_popup" name="addRepo">
