							<li><input type="checkbox" id="splitTest_{{id}}" class="splitTest" dynamicId="{{id}}"><a class="repo_tab testA" dynamicId="{{id}}">Test</a></li>
						</ul>
					</div>
					<div class="tab-content fixedscroll" id="addToRepoMyTabContent{{id}}">
