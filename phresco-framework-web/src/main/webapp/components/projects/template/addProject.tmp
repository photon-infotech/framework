<section id="addproject">    
    <section class="content_main create_proj">
		<div class="scrolldiv">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents"> 
            	<tr>
                    <td><span data-i18n="project.create.label.name"></span><sup>*</sup></td>
                    <td class="right-align">
                    	<input type="text" name="projectname" id="projectname" maxlength="30" data-i18n="[placeholder]project.create.label.projectname" title="30 Characters only"/>
                	</td>
                    <td><span data-i18n="project.create.label.code"></span><sup>*</sup></td>
                    <td><input type="text" name="projectcode" id="projectcode" maxlength="30" data-i18n="[placeholder]project.create.label.projectcode" title="30 Characters only"/></td>
					<td colspan=2>
                        <select class="custon_dropdown" name="builtmyself">
                            <option value="custom" data-i18n="project.create.label.custom"></option>
                            <option value="prebuilt" data-i18n="project.create.label.prebuilt"></option>
                        </select>
                        <select class="optional_dropdown" name="prebuiltapps">
                            <option data-i18n="project.create.label.selpilotproject"></option>
                        </select>
						<select class="optional_dropdown" name="builtmyselfapps" disabled>
                            <option>Select</option>
                        </select>
                    </td>
					<td rowspan="2" style="vertical-align: top;">
						<textarea class="popuptextarea" name="projectdescription" id="projectdescription" maxlength="150" title="150 Characters only" data-i18n="[placeholder]project.create.label.description"/>
					</td>
                </tr>
				<tr>
                    <td><span data-i18n="project.create.label.version"></span><sup>*</sup></td>
                    <td>
                    	<input type="text" name="projectversion" id="projectversion" readonly="readonly" style="cursor: pointer;" major="1" minor="0" fixed="0" iterationType="iteration" weekStart="1" value="1.0.0-SNAPSHOT"/>
                    	<img class="dropdown_icon" src="themes/default/images/Phresco/dropdown_icon.png" id="versionImg">
                	</td>
					<td><span data-i18n="project.create.label.groupid"></span></td>
                    <td><input type="text" name="groupId" id="groupId" maxlength="30" data-i18n="[placeholder]project.create.label.groupid" title="30 Characters only"/></td>
					<td><input type="checkbox" name="integrationTest" value="">Integration Test</td>
					<td></td>
				</tr>
				<tr>
                    <td data-i18n="project.create.label.startdate" name="startdate-lbl"></td>
                    <td class="startdate" name="startdateholder">
                    	<input name="startdate" type="text" id="startDate">
                		<span class="add-on"><img id="strdt" src="themes/default/images/Phresco/cal_icon.png" border="0" alt=""></span>
            		</td>
                    <td data-i18n="project.create.label.enddate" name="enddate-lbl"></td>
                    <td class="enddate" name="enddateholder">
                    	<input name="enddate" id="endDate" type="text" disabled>
                		<span class="add-on"><span class="add-on">
                			<img id="enddt" src="themes/default/images/Phresco/cal_icon.png" border="0" alt="">
            			</span>
        			</td>
					<td></td>
                </tr>
				{{#if applicationlayerData.techGroups}}
				<tr class="proj_title" name="applicationlayer">
					<td colspan="3">{{applicationlayerData.name}}</td>
					<td colspan="3"><span class="errmsg1"></span></td>
					<td colspan="2"><img name="close" id="applicationlayer" src="themes/default/images/Phresco/close_icon.png"  border="0" alt=""></td>
				</tr>
				<tr class="applnLayer" name="appvalidation" key="displayed">
					<td colspan="8" class="appln">
						<table class="table table-striped table_border border_div table-bordered applnlayer" cellpadding="0" cellspacing="0" border="0">
							<thead>
								<tr>
									<th><span data-i18n="project.create.label.application"></span><sup>*</sup></th>
									<th><span data-i18n="project.create.label.group"></span></th>
									<th><span data-i18n="project.create.label.technology"></span></th>
									<th><span data-i18n="project.create.label.version"></span></th>
									<th></th>
									<th></th>
								</tr>
							</thead>
							<tbody class="applnlayer">
								<tr class="applnlayercontent" name="staticApplnLayer" position="1">
									<td class="applnappcode"><input type="text" id="appcode"  maxlength="30" title="30 characters only" class="appln-appcode appCodeText" count=1></td>
									<td name="frontEnd" class="frontEnd">
										<select name="frontEnd" class="frontEnd selectpicker" title="Select Group">
											<option selected disabled value="">Select Group</option>
											{{#each applicationlayerData.techGroups}}
												<option value="{{id}}">{{name}}</option>
											{{/each}}
										</select>
									</td>
									<td name="technology" class="technology">
										<select name="appln_technology" class="appln_technology selectpicker" title="Select Technology">
										   <option disabled value="Select Technology">Select Technology</option>
										</select>
									 </td>
									 <td name="version" class="version">
										<select name="appln_version" class="appln_version selectpicker" title="Select Version">
										   <option disabled value="Select Version">Select Version</option>
										</select>
									 </td>
									<td>
										<input type="button" value="Multi Module" class="btn btn_style add_icon_btn multi_module_btn hideContent" name="multiModuleBtn">
									</td>
									 <td>
										<div class="flt_right icon_center">
											<a href="javascript:;" name="addApplnLayer" position="1"><img src="themes/default/images/Phresco/plus_icon.png" border="0" alt=""></a>
											<a href="javascript:;" name="removeApplnLayer" class="hideContent"><img src="themes/default/images/Phresco/minus_icon.png" border="0" alt=""/></a>
										</div>
									 </td>
							    </tr>
						  </tbody>
                       </table>
					</td>
				</tr>
				{{/if}}
				{{#if weblayerData.techGroups}}
				<tr class="proj_title" name="weblayer">
					<td colspan="3">{{weblayerData.name}}</td>
					<td colspan="3"><span class="errmsg2"></span></td>
					<td colspan="2"><img name="close" id="weblayer" src="themes/default/images/Phresco/close_icon.png"   border="0" alt=""></td>
				</tr>
				<tr class="webLayer" name="appvalidation" key="displayed">
					<td colspan="8" class="WebLayer">
						<table class="table table-striped table_border border_div table-bordered WebLayer" cellpadding="0" cellspacing="0" border="0">
							<thead>
								<tr>
									<th><span data-i18n="project.create.label.application"></span><sup>*</sup></th>
									<th><span data-i18n="project.create.label.group"></span></th>
									<th><span data-i18n="project.create.label.technology"></span></th>
									<th><span data-i18n="project.create.label.version"></span></th>
									<th></th>
									<th></th>
								</tr>
							</thead>
							<tbody class="WebLayer">
								<tr class="weblayercontent" name="staticWebLayer" position="1">
									<td class="webappcode"><input type="text" id="webappcode"  maxlength="30" title="30 characters only" class="web-appcode appCodeText" count=2></td>
									<td name="web" class="web">
										<select name="weblayer" class="weblayer selectpicker" title="Select Group">
											<option selected disabled value="Select Group">Select Group</option>
											{{#each weblayerData.techGroups}}
												<option value="{{id}}">{{name}}</option>
											{{/each}}
										</select>
									 </td>
									 <td name="widget" class="widget">
										<select name="web_widget" class="web_widget selectpicker" title="Select Technology">
											<option disabled value="Select Technology">Select Technology</option>
										 </select>
									 </td>
									 <td name="widgetversion" class="widgetversion">
										<select name="web_version" class="web_version selectpicker" title="Select Version">
											<option disabled value="Select Version">Select Version</option>
										</select>
									 </td>
									<td>
										<input type="button" value="Multi Module" class="btn btn_style add_icon_btn multi_module_btn hideContent" name="multiModuleBtn">
									</td>
									 <td>
										<div class="flt_right icon_center">
											<a href="javascript:;" name="addWebLayer" position="1"><img src="themes/default/images/Phresco/plus_icon.png" border="0" alt=""></a>
											<a href="javascript:;" name="removeWebLayer" class="hideContent"><img src="themes/default/images/Phresco/minus_icon.png" border="0" alt=""/></a>
										</div>
									 </td>
								</tr>
							</tbody>
						</table>
                    </td>
				</tr>
				{{/if}}
				{{#if mobilelayerData.techGroups}}
                    <tr class="proj_title" name="mobilelayer">
                      <td colspan="3">{{mobilelayerData.name}}</td>
                      <td colspan="3"><span class="errmsg3"></span></td>
                      <td colspan="2"><img name="close" id="mobilelayer" src="themes/default/images/Phresco/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" name="appvalidation" key="displayed">
                      	<td colspan="8" class="mob">
							<table class="table table-striped table_border border_div table-bordered mob-table" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.application"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.group"></span></th>
										<th><span data-i18n="project.create.label.technology"></span></th>
										<th><span data-i18n="project.create.label.version"></span></th>
										<th></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="cmsLayer">
									<tr class="mobilelayercontent" name="staticMobileLayer" position="1">
										<td class="mobileappcode"><input type="text" id="mobileappcode" maxlength="30" title="30 characters only" class="mobile-appcode appCodeText" count=3></td>
										<td name="mobile" class="mobile">
											<select name="mobile_layer" class="mobile_layer selectpicker" title="Select Model">
											   <option disabled selected value="Select Group">Select Group</option>
											   {{#each mobilelayerData.techGroups}}
												  <option value="{{id}}">{{name}}</option>
											   {{/each}}
											</select>
										 </td>
										 <td name="types" class="types">
											<select name="mobile_types" class="mobile_types selectpicker" title="Select Technology">
											   <option disabled value="Select Technology">Select Technology</option>
											</select>
										 </td>
										 <td name="mobileversion" class="mobileversion">
											<select name="mobile_version" class="mobile_version selectpicker" title="Select Version">
											   <option disabled value="Select Version">Select Version</option>
											</select>
										 </td>
										<td>
											<input type="button" value="Multi Module" class="btn btn_style add_icon_btn multi_module_btn hideContent" name="multiModuleBtn">
										</td>
										 <td>
											<div class="flt_right icon_center">
												<a href="javascript:;" name="addMobileLayer" position="1"><img src="themes/default/images/Phresco/plus_icon.png" border="0" alt=""></a>
												<a href="javascript:;" name="removeMobileLayer" class="hideContent"><img src="themes/default/images/Phresco/minus_icon.png" border="0" alt=""/></a>
											</div>
										 </td>
									</tr>
							   </tbody>
                           </table>
                        </td>
                     </tr>
					{{/if}}
              	</tbody>
            </table>
		</div>
    	<!--content_main div ends here -->
    </section>
	<div id="version_popup" style="display:none;" class="dyn_popup">
		<form id="versionForm">
			<table>
	            <tr>
	               	<td>Major</td>
					<td>Minor</td>
	                <td>Fixed</td>
	            </tr>
	            <tr>
	            	<td>
	                    <select id="majorVersion">
							<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
							<option value="6">6</option>
							<option value="7">7</option>
							<option value="8">8</option>
							<option value="9">9</option>
							<option value="10">10</option>
							<option value="11">11</option>
							<option value="12">12</option>
							<option value="13">13</option>
							<option value="14">14</option>
							<option value="15">15</option>
							<option value="16">16</option>
							<option value="17">17</option>
							<option value="18">18</option>
							<option value="19">19</option>
							<option value="20">20</option>
	                    </select>
	                </td>
	                <td>
	                	<select id="minorVersion">
	                		<option value="0">0</option>
	                       	<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
							<option value="6">6</option>
							<option value="7">7</option>
							<option value="8">8</option>
							<option value="9">9</option>
							<option value="10">10</option>
							<option value="11">11</option>
							<option value="12">12</option>
							<option value="13">13</option>
							<option value="14">14</option>
							<option value="15">15</option>
							<option value="16">16</option>
							<option value="17">17</option>
							<option value="18">18</option>
							<option value="19">19</option>
							<option value="20">20</option>
	                    </select>
	                </td>
	                <td>
	                	<select id="fixedVersion">
	                		<option value="0">0</option>
	                       	<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
							<option value="6">6</option>
							<option value="7">7</option>
							<option value="8">8</option>
							<option value="9">9</option>
							<option value="10">10</option>
							<option value="11">11</option>
							<option value="12">12</option>
							<option value="13">13</option>
							<option value="14">14</option>
							<option value="15">15</option>
							<option value="16">16</option>
							<option value="17">17</option>
							<option value="18">18</option>
							<option value="19">19</option>
							<option value="20">20</option>
	                    </select>
	                </td>
	            </tr>
				<tr>
					<td>Build Type</td>
				</tr>
				<tr>
					<td colspan="3">
	                    <select class="type_build" id="iterationType">
	                       	<option value="iteration">Iteration</option>
	                       	<option value="sprint">Sprint</option>
	                       	<option value="date">Date</option>
	                    </select>
	                </td>
				</tr>
				<tr>
					<td>Week Start</td>
				</tr>
				<tr>
					<td colspan="3">
	                    <select class="type_build" id="weekStart">
	                       	<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
							<option value="6">6</option>
							<option value="7">7</option>
							<option value="8">8</option>
							<option value="9">9</option>
							<option value="10">10</option>
							<option value="11">11</option>
							<option value="12">12</option>
							<option value="13">13</option>
							<option value="14">14</option>
							<option value="15">15</option>
							<option value="16">16</option>
							<option value="17">17</option>
							<option value="18">18</option>
							<option value="19">19</option>
							<option value="20">20</option>
	                    </select>
	                </td>
				</tr>
	        </table>
        </form>
        <div align="right">
        	<div class="flt_left">
	        	Ex : 1.0.0-SNAPSHOT
	        </div>
	        <div class="flt_right">
	        	<input type="button" class="btn btn_style dyn_popup_close" value="OK" id="submitVersion">
	        </div>
        </div>     
    </div>
    <section class="content_end">
    	<div class="flt_left">
        	<input type="submit" name="applicationlayer" value="{{applicationlayerData.name}}" class="btn btn_style app_layer_icon_btn hideContent">
            <input type="submit" name="weblayer" value="{{weblayerData.name}}" class="btn btn_style web_layer_icon_btn hideContent">
            <input type="submit" name="mobilelayer" value="{{mobilelayerData.name}}" class="btn btn_style mobile_layer_icon_btn hideContent">
        </div>
        <div class="flt_right">
        	<input type="submit" value="Create" data-i18n="[value]common.btn.create" name="Create" class="btn btn_style add_icon_btn">
            <input type="submit" value="Cancel" data-i18n="[value]common.btn.close" name="Cancel" class="btn btn_style cancel_icon_btn">
        </div>
		<div class="msgdisplay"></div>
    	<!--content_title ends here -->
    </section>
</section>	