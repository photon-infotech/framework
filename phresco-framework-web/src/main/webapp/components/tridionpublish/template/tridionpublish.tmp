 <section class="optiontitle">
        <div class="flt_left" >
			
			<font>Publishing</font>
		</div>
    </section>
	
<section class="tridion_info tab_line testSuiteTable">
<div class="scrolldiv">
<div class="fixed-table-container">
	<div class="header-background"><span class="classificationId" id="classificationId">Publishing</span></div>
	<div class="fixed-table-container-inner">
		<div class="scrolldiv no_border">
		<table class="table table-striped table_border table-hover table-bordered" cellpadding="0" cellspacing="0" border="0">
			<tbody name="ConfigurationLists"> 
				<tr>
					<td>Environment<sup>*</sup></td>
					<td>	
					{{envlist.data}}
						<select class="custon_dropdown" name="envList" id="envList">
							<option value="0">Select Env</option>
							{{#each envlist}}
								<option>{{name}}</option>
							{{/each}}
						</select>
					</td>
					<td>Environment Target<sup>*</sup></td>
					<td colspan="3">
						<select class="custon_dropdown" name="envTarget" id="envTarget" multiple>
						</select>
					</td>
				</tr>			
			</tbody>
		</table>

	</div>
</div>
</div>
</div>
</section>
<section class="content_end" name="configContent">
	<div class="flt_right">
		<input type="submit" name="unpublish" id="unpublish" value="Unpublish" class="btn btn_style save_btn">
		<input type="submit" name="publish" id="publish" value="Publish" class="btn btn_style">
	</div>
	<div class="msgdisplay"></div>
	<!--content_title ends here -->
</section>		    
